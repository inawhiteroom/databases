-- Скрипты характерных выборок:

-- Вывести количество отелей в каждой стране:
select country, count(*) from booking_com.hotels group by country;
-- Вывести список отелей и их уровень согласно рейтингу:
use booking_com;
select h.hotel_name as hotel, s.star as star
from hotels as h
join stars as s on h.id = s.hotel_id;
-- Вывести список забронированных номеров и гостей, осуществивших это бронирование
select
room_id,
(select surname from guests where guest_id = id) as guest_name 
from booking;

-- Представления

-- Вывести список отелей и соответствующие им типы номеров:
create view room_info
as select hotel_name, `type`
from rooms, hotels
where hotel_id = hotels.id;

select * from room_info;
-- Вывести сообщения, отправленные гостями и имена этих гостей:
create view guests_messages
as select body, `name`, surname
from messages, guests
where from_guest_id = guests.id;

select * from guests_messages;

-- Хранимые процедуры / триггеры

-- Процедура, показывающая список отелей в стране
DELIMITER //
create procedure hotel_country (country_arg varchar(45))
begin 
	select hotel_name, country
    from hotels
    where country = country_arg;
end//
DELIMITER ;

call hotel_country ('Indonesia');
-- Триггер, рассчитывающий скидку 30%
DELIMITER //
create trigger before_discount_30
before update on rooms
for each row
begin
	set new.price = new.price * 0.7;
end //
DELIMITER ;


