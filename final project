  -- 4
  
  DROP TABLE IF EXISTS `messages`;
  CREATE TABLE `messages` (
  `id` bigint unsigned NOT NULL AUTO_INCREMENT,
  `from_guest_id` bigint unsigned NOT NULL,
  `to_hotel_id` bigint unsigned NOT NULL,
  `from_hotel_id` bigint unsigned NOT NULL,
  `to_guest_id` bigint unsigned NOT NULL,
  `body` text,
  PRIMARY KEY (`id`),
  UNIQUE KEY `id` (`id`),
  KEY `from_guest_id` (`from_guest_id`),
  KEY `to_hotel_id` (`to_hotel_id`),
  KEY `from_hotel_id` (`from_hotel_id`),
  KEY `to_guest_id` (`to_guest_id`),
  CONSTRAINT `messages_ibfk_1` FOREIGN KEY (`from_guest_id`) REFERENCES `guests` (`id`),
  CONSTRAINT `messages_ibfk_2` FOREIGN KEY (`to_hotel_id`) REFERENCES `hotels` (`id`),
  CONSTRAINT `messages_ibfk_3` FOREIGN KEY (`from_hotel_id`) REFERENCES `hotels` (`id`),
  CONSTRAINT `messages_ibfk_4` FOREIGN KEY (`to_guest_id`) REFERENCES `guests` (`id`));
  
